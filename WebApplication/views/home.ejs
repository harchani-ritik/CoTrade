<!DOCTYPE html>
<html>
<head>
	<title>signin</title>
</head>
<body>
	<header>
		<span>logo</span>
		<span><a href="">Home</a></span>
		<span>Feed</span>
		<span>connections</span>
		<span class="active">Profile</span>
	</header>

	<div>
		<div><input type="search" name="search"></div>
		<div>
			<table>
				<tr>
					<th>Stock</th>
					<th>Purchase price <br>(Rs.)</th>
					<th>Shares</th>
					<th>Time</th>
				</tr>
				<% maping.forEach(function(value, key) { %>		
						<form method="POST" action="/stock_card">
							<tr>
								<th><button name="key" value="<%= key %>"><%= key %></button></th>
								<th><%= value.current_price %></th>
								<th><%= value.open_price %></th>
								<th><%= (value.current_price - value.open_price) / value.open_price %></th>
							</tr>
						</form>
				<% }) %>
			</table>
		</div>
	</div>

</body>
</html>